<div>
    <h1 class="card center-align">News from THE SUN</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card center-align col s6">Click to scrape</h5>
    </div>
</div>

<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this._id}}" id="btnSignup">Read article</a>
        </div>
        {{/each}}
    </div>
</div>